    <property name="now" value="now()" dbms="h2"/>
    
    <!--
        Added the entity VcutUserDeviceMaster.
    -->
    <changeSet id="20190930115512-1" author="jhipster">
        <createTable tableName="vcut_user_device_master">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="user_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="created_by" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="created_date" type="datetime">
                <constraints nullable="true" />
            </column>

            <column name="vcut_device_line_master_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        <createSequence startValue="1" sequenceName="vcut_user_device_master_seq" incrementBy="1" cycle="false"/>
        <dropDefaultValue tableName="vcut_user_device_master" columnName="created_date" columnDataType="datetime"/>
    </changeSet>

    <changeSet id="20190930115512-1-relations" author="jhipster">

    </changeSet>
